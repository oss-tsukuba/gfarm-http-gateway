{
  "name": "gfarm-http-gateway-webui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.3.8",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.1",
    "virtua": "^0.43.1"
  },
  "scripts": {
    "start": "vite --mode development --no-open",
    "build": "vite build --mode production",
    "lint": "eslint ./src --ext .js,.jsx",
    "lint:fix": "eslint ./src --ext .js,.jsx --fix",
    "e2e:all": "playwright test",
    "e2e:chromium": "playwright test --project=chromium",
    "e2e:firefox": "playwright test --project=firefox",
    "e2e:webkit": "playwright test --project=webkit",
    "e2e:ui": "playwright test --project=chromium --ui",
    "e2e:ui:firefox": "playwright test --project=firefox --ui",
    "e2e:ui:webkit": "playwright test --project=webkit --ui",
    "e2e:headed": "playwright test --project=chromium --headed"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.0",
    "@vitejs/plugin-react": "^5.0.2",
    "adm-zip": "^0.5.16",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "vite": "^7.1.5",
    "vite-jsconfig-paths": "^2.0.1",
    "web-vitals": "^5.1.0"
  }
}
